{
  "name": "apache-autoindex-parse",
  "version": "4.1.0",
  "description": "parse apache's autoindex html files",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.24.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/apache-autoindex-parse",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/apache-autoindex-parse.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/apache-autoindex-parse/issues"
  },
  "keywords": [
    "autoindex",
    "parse",
    "apache"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./test-utils": {
      "import": "./dist/test-utils.mjs",
      "require": "./dist/test-utils.cjs"
    },
    "./traverse": {
      "import": "./dist/traverse.mjs",
      "require": "./dist/traverse.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "6.0.3",
    "@types/node": "22.15.33",
    "eslint": "9.39.1",
    "eslint-plugin-format": "1.1.0",
    "publint": "0.3.15",
    "testcontainers": "11.9.0",
    "tsdown": "0.17.0",
    "typescript": "5.9.3",
    "vitest": "4.0.15",
    "vitest-testdirs": "4.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
